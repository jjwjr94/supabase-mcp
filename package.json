{
  "name": "@jjwjr94/supabase-mcp-http",
  "version": "0.5.3",
  "description": "Supabase MCP Server with HTTP streaming support for Render deployment",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjwjr94/supabase-mcp.git"
  },
  "homepage": "https://github.com/jjwjr94/supabase-mcp#readme",
  "main": "dist/index-http-simple.js",
  "type": "module",
  "bin": {
    "supabase-mcp-http": "dist/index-http-simple.js"
  },
  "files": [
    "dist",
    "packages",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index-http-simple.js",
    "start:http": "node dist/index-http-simple.js",
    "dev": "NODE_ENV=development NODE_OPTIONS='--loader ts-node/esm' node --experimental-specifier-resolution=node index-http-simple.ts",
    "dev:http": "NODE_ENV=development NODE_OPTIONS='--loader ts-node/esm' node --experimental-specifier-resolution=node index-http-simple.ts",
    "dev:secure": "NODE_ENV=development NODE_OPTIONS='--loader ts-node/esm' node --experimental-specifier-resolution=node index-http-secure.ts",
    "test": "node test-http-server.js",
    "watch": "tsc -w",
    "prepublishOnly": "npm run build",
    "inspector": "npx @modelcontextprotocol/inspector dist/index-http.js"
  },
  "keywords": [
    "supabase",
    "mcp",
    "anthropic",
    "http",
    "streaming",
    "render"
  ],
  "author": "Jay Wong",
  "license": "Apache-2.0",
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.4.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.10.1",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.0",
    "@mjackson/multipart-parser": "^0.10.1",
    "common-tags": "^1.8.2",
    "graphql": "^16.11.0",
    "openapi-fetch": "^0.13.5",
    "zod": "^3.24.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0"
  }
}